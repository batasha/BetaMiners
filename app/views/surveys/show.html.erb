<p>
  <strong>Name:</strong>
  <%= "#{@survey.project.name} #{@survey.test_phase.name} Feedback" %>
</p>

<ol>
  <% @survey.questions.each do |question| %>
  <li><%= question.text %></li>
  <ul>
    <% question.responses.each do |response| %>
      <li><%= response.text %></li>
    <% end %>
  </ul>
  <% end %>
</ol>

